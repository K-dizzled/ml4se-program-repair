{
  "name": "code-repair",
  "displayName": "Program Repair",
  "description": "Utilizing Large Language Models to preair a dataset and use it to train a model to repair code.",
  "repository": {
    "type": "git",
    "url": "https://github.com/K-dizzled/pure-ts-template"
  },
  "version": "0.0.1",
  "main": "./out/mainNode.js",
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "format": "prettier --write \"src/**/*.{ts,js}\" && eslint \"src/**/*.{ts,js}\" --fix",
    "clean": "rm -rf out",
    "serve": "node ./out/mainNode.js",
    "preserve": "npm run rebuild",
    "rebuild": "npm run clean && npm run compile",
    "test": "jest",
    "pretest": "npm run format && npm run rebuild"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/cli-progress": "^3.11.3",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "22.10.2",
    "@types/tmp": "^0.2.6",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "earl": "^1.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.9.1",
    "axios": "^1.6.2",
    "cli-progress": "^3.12.0",
    "dedent": "^1.5.1",
    "dotenv": "^16.4.5",
    "event-source-polyfill": "^1.0.31",
    "i": "^0.3.7",
    "npm": "^10.4.0",
    "path": "^0.12.7",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "pybridge": "^1.4.0",
    "toml": "^3.0.0",
    "ts-node": "^10.9.2",
    "ts-results": "^3.3.0",
    "yargs": "^17.7.2"
  }
}